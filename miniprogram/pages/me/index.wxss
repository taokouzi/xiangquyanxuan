.ewmshare{
    width: 250rpx;
    height: 250rpx;
    position: absolute;
    right:36rpx;
    bottom:-110rpx;
    animation:upDwon 3s ease-in-out .1s infinite;
    -webkit-animation:upDwon 3s ease-in-out .1s infinite;
}
@keyframes upDwon{
    0%{
        transform: translateY(0);
        -webkit-transform: translateY(0);
    }
    50%{
        transform: translateY(4rpx);
        -webkit-transform: translateY(4rpx);
    }
    100%{
        transform: translateY(0);
        -webkit-transform: translateY(0);
    }
}
@-webkit-keyframes upDwon{
    0%{
        transform: translateY(0);
        -webkit-transform: translateY(0);
    }
    50%{
        transform: translateY(4rpx);
        -webkit-transform: translateY(4rpx);
    }
    100%{
        transform: translateY(0);
        -webkit-transform: translateY(0);
    }
}
